[java]
    jar_spool_mode = direct_to_jar
    src_roots = java, resources
    extra_arguments =  -encoding, UTF-8

[project]
    ignore = .git, eclipse-out/
    parallel_parsing = true

[cache]
    mode = dir
    dir = buck-out/cache

[buildfile]
    includes = //DEFS

[cache]
    mode = dir
    dir = ~/.minorminor/buck-cache/local
